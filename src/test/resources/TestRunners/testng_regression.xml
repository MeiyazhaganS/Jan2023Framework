<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Online Cart Test Suite" verbose="4" thread-count="2" parallel="tests">

	<listeners>
		<listener class-name="com.cart.listeners.ExtentReportListener" />
		<listener class-name="com.cart.listeners.TestAllureListener" />
		<!-- <listener class-name="com.cart.listeners.AnnotationTransformer" />-->
	</listeners>

	  <test name="Online Cart Login Page Test">
		<classes>
			<class name="com.cart.pagetest.Loginpagetest" />
		</classes>
	</test> 

	<test name="Online Cart Account Page Test">
		<classes>
			<class name="com.cart.pagetest.Accountpagetest" />
		</classes>
	</test>
	
</suite>